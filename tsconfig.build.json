{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "declaration": false,
    "declarationMap": false,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": [],
    //    "jsx": "react-jsx",
    "noEmit": false
  },
  "exclude": ["node_modules", "dist"]
}
