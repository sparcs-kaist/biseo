{
  "name": "biseo-server",
  "version": "2.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "dev:db": "docker compose -f docker-compose.dev.yml up",
    "dev:db:up": "docker compose -f docker-compose.dev.yml up -d",
    "dev:db:down": "docker compose -f docker-compose.dev.yml down",
    "dev:db:migrate": "prisma migrate dev",
    "build": "tsc",
    "env": "echo $NODE_ENV"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "ldapts": "^4.2.6",
    "socket.io": "^4.5.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.18",
    "biseo-interface": "*",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.4"
  }
}
