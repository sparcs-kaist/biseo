{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@biseo/interface": [
        "../../interface/src"
      ],
      "@biseo/interface/*": [
        "../../interface/src/*"
      ]
    },
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}
